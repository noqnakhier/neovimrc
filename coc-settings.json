{
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"]
        }
    },

    "go.goplsOptions": {
        "completeUnimported": true
    },
    "go.tags": {
        "options": "json=",
        "transform":"camelcase"
    },

    "python.jediEnabled": false
}

